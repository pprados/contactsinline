<resources  xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

	<!-- No error -->
	<string name="noException"/>

	<!-- Truncate the response to limit the memory usage or it's a limitation of the remote serveur-->
	<string name="err_truncated">Réponse tronquée. Erreur réseau ou limitation du serveur.</string>

	<!-- The network is done. It's not necessary to try a request. -->
	<string name="err_network_done">Réseau absent. Essayez plus tard</string>
	
	<!-- The version of the extension is incompatible with the version of "contact inline" -->
	<string name="err_invalide_version">Mauvaise version. Mise a jour de l\'application et des extensions.</string>

	<!-- The toke type for authentification is invalid -->
	<string name="err_invalide_token_type">Type d\'authentification invalide.</string>

	<!-- Connexion error. -->
	<string name="err_unknown">Erreur de connexion.</string>

	<!-- Authentification is rejected. -->
	<string name="err_authent">Authentification invalide.</string>

	<!-- Error when administrator in LDAP server put a privileges -->
	<string name="err_access_right">Erreur de login dûe à un problème de privilèges.</string>

	<!-- Error when administrator in LDAP server put a limit -->
	<string name="err_admin_limit_exceeded">Atteinte des limitations de l\'administrateur.</string>

	<!-- Response size is to big. -->
	<string name="err_size_exceeded">Réponse tronquée. La taille est excessive.</string>

	<!-- The remote server is down. -->
	<string name="err_server_down">Le serveur ne répond pas</string>

	<!-- The time limit to receive a response is exceeded. -->
	<string name="err_time_limit_exceeded">Temps limite dépassé.</string>

	<!-- The number of record in the response is exceeded. -->
	<string name="err_maxitem_exceeded">Réponse tronquée. Nombre d\'enregistrement trop important.</string>

	<!-- Impossible to establish connection. -->
	<string name="err_connection_exception">Impossible d\'établir la connexion.</string>

	<!-- Error with the network -->
	<string name="err_io_exception">Erreur réseau.</string>

	<!-- Beta version is expired. -->
	<string name="err_beta_expired">Beta expirée.</string>

</resources>
